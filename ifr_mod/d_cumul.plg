set key left
set grid
set xrange [0:*]
set xlabel 'days since 1.1.2020'
set ylabel 'cumulative deaths'
set xtics 0,14



plot "d_alld.dat" u 1:($1>0 ? $2 : 0) smooth cumulative w l lw 2 title 'reported',"d_allp.dat" u ($1+3):($1>0 ? $2 : 0) smooth cumulative w l lw 2 title 'predicted (t+3)',"d_allp.dat" u ($1+3):($1>0 ? 1.2*($2) : 0) smooth cumulative w l lw 2 title 'predicted+20% (t+3)'
